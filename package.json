{
  "name": "web-scaffold",
  "version": "0.0.0",
  "description": "An example web scaffold",
  "main": "server.js",
  "scripts": {
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text npm test -- --reporter min",
    "lint": "eslint ./src ./test ./build ./config ./server.js",
    "lint:fix": "eslint --fix ./src ./test ./build ./config./server.js",
    "flow": "flow",
    "flowtypes": "flow-typed install --overwrite",
    "dev": "node ./build/index.js dev",
    "prod": "node ./build/index.js prod",
    "start": "node server.js",
    "test": "npm run test:unit && npm run test:acceptance",
    "test:acceptance": "mocha --opts test/acceptance/mocha.opts",
    "test:unit": "mocha --opts test/unit/mocha.opts",
    "package": "node ./build/index.js package",
    "deploy": "node ./build/index.js deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhardy/web-scaffold.git"
  },
  "author": "Nathan Hardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhardy/web-scaffold/issues"
  },
  "homepage": "https://github.com/nhardy/web-scaffold#readme",
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "build",
      "coverage",
      "dist",
      "flow",
      "flow-typed",
      "node_modules",
      "test",
      "server.js"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "all": true
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/node": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-flow": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "app-module-path": "^2.2.0",
    "archiver": "^2.1.1",
    "autoprefixer": "^8.1.0",
    "azur": "^0.0.5",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "buffer-loader": "0.0.1",
    "cache-loader": "^1.2.2",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-http": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.0",
    "eslint-config-nhardy": "^0.3.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0",
    "glob": "^7.1.2",
    "httpolyglot": "^0.1.2",
    "jsdom": "^11.6.2",
    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.4",
    "mocha-junit-reporter": "^1.17.0",
    "nock": "^9.2.3",
    "node-hook": "^1.0.0",
    "nodemon": "^1.17.2",
    "nyc": "^11.6.0",
    "postcss-loader": "^2.1.2",
    "proxyquire": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.2.0",
    "selfsigned": "^1.10.2",
    "sinon": "^4.4.6",
    "string-hash": "^1.1.3",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.1.0",
    "text-loader": "^0.0.1",
    "thread-loader": "^1.1.5",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "@babel/runtime": "^7.0.0-beta.42",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "geojson2svg": "^1.2.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash-es": "^4.17.7",
    "nodemailer": "^4.6.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-display-name": "^0.2.3",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-router-scroll": "^0.4.4",
    "redux": "^3.7.2",
    "redux-connect": "^6.0.0",
    "redux-scripts-manager": "^0.3.3",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.5.4",
    "ua-parser-js": "^0.7.17"
  },
  "engines": {
    "node": "^9.0.0",
    "npm": "^5.6.0"
  }
}
